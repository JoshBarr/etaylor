	<meta property="og:image"           content="{{ album.image|default("") }}" /> 
	{% endif %}

	<meta property="cookbook:author"    content="http://samples.ogp.me/390580850990722" />
	<meta name="viewport" content="width=device-width, initial-scale=1">

	{% block meta %}
	{% endblock %}
	{% block css %}
    <link rel="stylesheet" href="{{ url_for('static', filename='css/screen.css') }}">
    {% endblock %}
	<link rel="shortcut icon" href="{{ url_for('static', filename='favicon.ico') }}">

	<title>E-taylor :: Up</title>

